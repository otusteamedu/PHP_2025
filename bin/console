#!/usr/bin/env php
<?php
//chmod +x bin/console

require __DIR__ . '/../vendor/autoload.php';

use App\Infrastructure\Console\CommandEvent\StorageAddTestDataCommand;
use App\Infrastructure\Console\CommandEvent\StorageSearchCommand;
