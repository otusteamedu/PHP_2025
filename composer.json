{
    "name": "elasticsearch/bookstore",
    "description": "Bookstore search with Elasticsearch",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "elasticsearch/elasticsearch": "^8.0",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Andrey Yurchuk",
            "email": "a.yurchuk1430@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
