{
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "ext-pdo": "*",
    "ext-redis": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\": "database/"
    },
    "classmap": [
      "database/"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}