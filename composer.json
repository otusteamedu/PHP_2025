{
    "name": "mdmitriev/hw10",
    "type": "project",
    "description": "Learn project",
    "proprietary": "MIT",
    "require": {
        "elasticsearch/elasticsearch": "^8.17",
        "ext-redis": "*",
        "symfony/dotenv": "^7.2",
        "symfony/console": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/yaml": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2",
        "phpunit/phpunit": "^11.5",
        "codeception/codeception": "^5.3",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-rest": "^3.4",
        "codeception/module-asserts": "^3.2",
        "codeception/module-symfony": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "phpunit",
        "cest-build": "codecept build",
        "cest-run": "codecept run --verbose"
    }
}
