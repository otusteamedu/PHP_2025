{
    "index": "otus-shop",
    "body": {
        "query": {
            "bool": {
                "must": [
                    {
                        "multi_match": {
                            "query": "",
                            "fields": [
                                "title"
                            ],
                            "fuzziness": "AUTO"
                        }
                    }
                ],
                "filter": [
                    {
                        "bool": {
                            "should": [
                                {
                                    "range": {
                                        "stock.stock": {
                                            "gt": 1
                                        }
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        }
    }
}