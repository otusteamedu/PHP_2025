{
  "name": "igainutdinov/example-application",
  "version": "0.0.1",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Base\\": "base/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "psr/container": "@stable",
    "psr/cache": "@stable",
    "psr/http-message": "@stable",
    "psr/log": "@stable",
    "ext-pdo": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12"
  },
  "scripts": {
    "test": [
      "@php vendor/bin/phpunit"
    ]
  }
}
